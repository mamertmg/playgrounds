<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;200;300;400&display=swap" rel="stylesheet" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
  <script src="https://kit.fontawesome.com/dcfa7fe939.js" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
  <title>Playground Details</title>
  <link rel="stylesheet" href="/css/detailpage.css" />
</head>

<body>
  <nav id="mainNavbar" class="navbar navbar-dark navbar-expand-md py-0 px-3 fixed-top">
    <a id="nav-header" class="navbar-brand">
      <!-- Link to fulfill license regulations of flaticon.com:
                  <a href="https://www.flaticon.com/free-icons/playground" title="playground icons">Playground icons created by
        photo3idea_studio - Flaticon</a> -->

      <img id="navbar-logo" src="/images/playground.png" alt="" /> my favourite playground
    </a>
    <button class="navbar-toggler" data-toggle="collapse" data-target="#navlinks" aria-label="Toggle Navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div id="navlinks" class="collapse navbar-collapse">
      <ul class="navbar-nav">
        <li class="nav-item">
          <a href="/" class="nav-link">HOME</a>
        </li>
        <li class="nav-item">
          <a href="/playgrounds" class="nav-link">EXPLORE</a>
        </li>
        <li class="nav-item">
          <a href="" class="nav-link">ABOUT</a>
        </li>
      </ul>
    </div>
    <div class="text-right d-none d-lg-block">
      <% if(isAuthenticated){ %>
      <a class="nav-link" href="/logout">LOGOUT</a>
      <% } %>
      <% if(!isAuthenticated){ %>
      <a id="btn-login" class="btn p-1 mx-1" href="/login">LOGIN</a>
      <a id="btn-signup" class="btn p-1 mx-1" href="/signup">SIGN UP</a>
      <% } %>
    </div>
  </nav>

  <section class="container-fluid p-0 p-xs-2 p-sm-4 p-md-5">
    <div class="row">
      <h1 class="gx-5 col" id="pgName">
        <%= playground.name %>
        <sup>
          <span class="playground-rating">
            <i class="bi bi-star-fill"></i>
            <i class="bi bi-star-fill"></i>
            <i class="bi bi-star-fill"></i>
            <i class="bi bi-star-fill"></i>
            <i class="bi bi-star-half"></i>
          </span>
        </sup>
      </h1>
      <div class="col d-flex justify-content-end">
        <% if(isAuthenticated && currentUser._id.toString()===playground.author.toString()){ %>
        <span><a class="btn btn-warning me-2" href="/playgrounds/<%= playground._id %>/edit">Update</a></span>
        <form style="display: inline;" action="/playgrounds/<%= playground._id %>?_method=delete" method="post">
          <input type="hidden" name="_csrf" value="<%= csrftoken %>">
          <button class="btn btn-danger">Delete</button>
        </form>
        <% } %>
      </div>
    </div>
    <div class="row">
      <div class="gx-5 col">
        <div class="btn tag">
          <i class="fa-solid fa-leaf"></i>
          <%= playground.type %>
        </div>
        <div class="btn tag">
          <i class="fa-solid fa-location-dot"></i>
          <%= playground.suburb %>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="gx-5 col-md-6 order-md-2 mb-3">
        <div id="playgroundDetails" class="card-body">


          <h3 class="card-title">
            <%= playground.suburb.toUpperCase() %>, <%= playground.city %>
          </h3>
          <a class="btn-collapse" data-bs-toggle="collapse" href="#infocard-collapse" role="button" aria-expanded="false" aria-controls="infocard-collapse">
            <i class="fa-solid fa-map-location-dot"> </i>
          </a>
          <p class="card-text">
          <p>
          <h5>AGE-RANGE:</h5> <%= playground.min_age %> - <%= playground.max_age %></p>
          <h5>DESCRIPTION:</h5> </b>
          <%= playground.description %>
          </p>


          <h5>FEATURES</h5>
          <div id="featureTags">
            <% for(label of playground.labels) { %>
            <div class="btn littleTags">
              <i class="fa-solid <%= labelToIcon[label] ? labelToIcon[label] : labelToIcon.default %>"></i>
              <span class="">
                <%= label %>
              </span>
            </div>
            <% } %>
          </div>


          <div class="collapse" id="infocard-collapse">
            <iframe src="https://maps.google.com/maps?hl=en&amp;q=<%= playground.location.coordinates[1] %>,%20<%= playground.location.coordinates[0] %>&amp;t=&amp;z=15&amp;ie=UTF8&amp;iwloc=B&amp;output=embed" width="100%" height="300vh" style="border:2px;" allowfullscreen="" loading="lazy">
            </iframe>
          </div>
        </div>
      </div>

      <div class="gx-5 col-md-6 order-md-1">



        <!-- Modal -->
        <div class="modal fade" id="addPhotoForm" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="addPhotoFormLabel" aria-hidden="true">
          <div class="modal-dialog">
            <div class="modal-content p-3">
              <div class="modal-header">
                <h4 class="modal-title" id="staticBackdropLabel">
                  Add a photograph
                </h4>
                <button type="button" id="btnClose5" class="btn-close btnClose" data-bs-dismiss="modal" aria-label="Close"></button>
              </div>
              <div class="modal-body">
                <form id="photoForm" class="mb-5" action="https://google.com">
                  <h5 class="mt-3 mb-2"></h5>
                  <input class="form-control" type="file" id="playgroundPhoto" onchange="preview3()" required />
                  <img id="framePhoto" src="" class="img-fluid mt-4" />
                  <label class="form-check-label" for="photoDescription">
                    Description
                  </label>
                  <input id="photoDescription" type="text" class="w-100 mt-0 mb-2 p-2 br-10" placeholder="Give a short description" />
                </form>
              </div>
              <span>
                <h3 class="text-center p-5" id="submitMessagePhoto">
                  Thank you! Your photo has been added!
                </h3>
              </span>

              <div class="modal-footer">
                <button type="button" id="btnClose6" class="btn btn-secondary btnClose" data-bs-dismiss="modal">
                  Close
                </button>
                <button id="btnSubmitPhoto" type="submit" form="photoForm" class="btn btn-primary">
                  Send
                </button>
              </div>
            </div>
          </div>
        </div>

        <div id="carouselIndicators" class="carousel slide" data-bs-ride="carousel">
          <div class="carousel-indicators">
            <button type="button" data-bs-target="#carouselIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
            <button type="button" data-bs-target="#carouselIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
            <button type="button" data-bs-target="#carouselIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
          </div>

          <div class="carousel-inner">
            <div class="carousel-item active">
              <img src="https://images.unsplash.com/photo-1606733894347-7cb201dc810b?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1332&q=80" class="d-block w-100 img-fluid" alt="" />
            </div>
            <div class="carousel-item">
              <img src="https://images.unsplash.com/photo-1575783970733-1aaedde1db74?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1176&q=80" class="d-block w-100 img-fluid" alt="" />
            </div>
            <div class="carousel-item">
              <img src="https://images.unsplash.com/photo-1596997000103-e597b3ca50df?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1074&q=80" class="d-block w-100 img-fluid" alt="" />
            </div>
          </div>

          <button class="carousel-control-prev" type="button" data-bs-target="#carouselIndicators" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
          </button>
          <button class="carousel-control-next" type="button" data-bs-target="#carouselIndicators" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
          </button>
          <a id="addphoto" class="btn btn-plus position-absolute top-0 end-0" data-bs-toggle="modal" data-bs-target="#addPhotoForm">
            <svg xmlns="http://www.w3.org/2000/svg" width="50" height="50" fill="currentColor" class="bi bi-plus-square-fill align-self-end" viewBox="0 0 16 16">
              <path d="M2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2zm6.5 4.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3a.5.5 0 0 1 1 0z" />
            </svg>
          </a>
        </div>

      </div>

      <div class="w-100 d-none d-md-block d-xl-none">
      </div>
      <div class="gx-5 col-12 col-xl-6 order-md-3 order-xl-4">
        <h3 id="event-header">UPCOMING EVENTS</h3>
        <% if(isAuthenticated) { %>
        <span class="d-inline-block" tabindex="0" data-bs-toggle="popover" data-bs-trigger="hover focus" data-bs-content="Add an Event">
          <!-- Button trigger modal -->

          <span id="createEventBtn" class="btn" data-bs-toggle="modal" data-bs-target="#staticBackdrop">
            <a class="btn-plus"><svg xmlns="http://www.w3.org/2000/svg" width="50" height="50" fill="currentColor" class="bi bi-plus-square" viewBox="0 0 25 25">
                <path d="M14 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z" />
                <path d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z" />
              </svg>
            </a>
          </span>
        </span>

        <!-- Modal -->
        <div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
          <div class="modal-dialog">
            <div class="modal-content p-3">
              <div class="modal-header">
                <h4 class="modal-title" id="staticBackdropLabel">
                  Add an Event
                </h4>
                <button type="button" id="btnClose1" class="btn-close btnClose" data-bs-dismiss="modal" aria-label="Close"></button>
              </div>
              <div id="eventForm" class="modal-body">
                <form id="actualEventForm" class="mb-5" action="/playgrounds/<%= playground._id %>/event" method="post">
                  <input type="hidden" name="_csrf" value="<%= csrftoken %>">
                  <label class="form-check-label" for="eventDate">
                    Date
                  </label>
                  <input id="eventDate" name="event[date]" type="date" class="w-100 mt-0 mb-2 p-2 br-10" required />
                  <label class="form-check-label" for="eventTime">
                    Time
                  </label>
                  <input id="eventTime" name="event[time]" type="time" class="w-100 mt-0 mb-2 p-2 br-10" required />
                  <label class="form-check-label" for="eventName">
                    Name of the event
                  </label>
                  <input id="eventName" name="event[title]" type="text" class="w-100 mt-0 mb-2 p-2 br-10" placeholder="What are you planning?" required />
                  <label class="form-check-label" for="eventInfo">
                    Info
                  </label>
                  <textarea id="eventInfo" name="event[description]" class="w-100 mt-0 mb-2 p-2 br-10" placeholder="add some details about the event" cols="30" rows="2" required></textarea>

                  <label class="form-check-label" for="eventLink">
                    Add a link
                  </label>
                  <input id="eventLink" name="event[link]" type="link" class="w-100 mt-0 mb-2 p-2 br-10" placeholder="e.g. event homepage" />

                  <h5 class="mt-3 mb-2">
                    Create a thumbnail for your event!
                  </h5>
                  <input class="form-control" type="file" id="eventPhoto" onchange="preview()" />
                  <img id="frame" src="" class="img-fluid mt-4" />

                </form>
              </div>
              <span>
                <h3 class="text-center p-5" id="submitMessage">
                  Thank you! Your event has been added.
                </h3>
              </span>

              <div class="modal-footer">
                <button type="button" id="btnClose2" class="btn btn-secondary btnClose" data-bs-dismiss="modal">
                  Close
                </button>
                <button id="btnSubmit" type="submit" class="btn btn-primary" form="actualEventForm">
                  Send
                </button>
              </div>
            </div>
          </div>
        </div>
        <% } %>

        <% if(!playground.events.length) { %>
        <div style="height: 60px; text-align: center;">
          No Events
        </div>
        <% } else { %>
        <div class="card">
          <ul>
            <% playground.events.forEach(function(event, i) {
                const date = new Date(event.date); %>
            <li>
              <div class="row link" data-bs-toggle="collapse" href="#event<%= i %>-collapse" role="button" aria-expanded="false" aria-controls="event<%= i %>-collapse">

                <div id="theDate" class="col-2 text-center">
                  <div class="day d-inline-block">
                    <%= date.getDate() %>
                  </div>
                  <div class="month text-center">
                    <%= numToMonth[date.getMonth()] %>
                  </div>

                </div>
                <div class="col text-center">
                  <h5 class="heading">
                    <%= event.title %>
                  </h5>
                  <div class="time">
                    <%= numToDay[date.getDay()] %>, <%= [date.getHours(), date.getMinutes()].join(":") %>
                  </div>
                  <div class="location">
                    <%= playground.name %>
                  </div>

                </div>


                <div class="col-3">

                  <img class="thumbnail" src="https://images.unsplash.com/photo-1615445565741-c60a9edd393f?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxzZWFyY2h8N3x8a2lkcyUyMHBhcnR5fGVufDB8fDB8fA%3D%3D&auto=format&fit=crop&w=500&q=60" alt="">
                </div>
                <% if(isAuthenticated && currentUser._id.toString()===event.author.id.toString()) { %>
                <div class="col-2 justify-content-center align-self-center">


                  <button data-id="<%= event._id %>" id="editEventButton" class="btn bi bi-pencil inline-block editEventBtn">
                  </button>
                  <label class="d-none d-md-inline" for="editEventButton">Edit</label>
                  <form action="/playgrounds/<%= playground._id %>/event/<%= event._id %>?_method=DELETE" method="post">
                    <input type="hidden" name="_csrf" value="<%= csrftoken %>">
                    <button id="deleteEventButton" class="btn bi bi-trash3 inline-block deleteEventBtn"></button>
                    <label class="d-none d-md-inline" for="deleteEventButton">Delete</label>
                  </form>

                </div>
                <% } %>

                <div class="row collapse mt-3" id="event<%= i %>-collapse">
                  <hr>
                  <div class="col-2 text-center align-self-center">
                    <img src="https://i.imgur.com/C4egmYM.jpg" height="50" width="50" class="rounded-circle" />
                    <p> <%= event.author.name %></p>
                  </div>
                  <div class="col text-justify align-self-center">
                    <%= event.description %>
                  </div>
                  <div class="col-3 align-self-center">
                    <a href="<%= event.link %>">
                      <%= event.link %>
                    </a>
                  </div>

            </li>
            <% }); %>
          </ul>
        </div>
        <% } %>



        <h3 id="lostfound-header">LOST & FOUND</h3>
        <% if(isAuthenticated) { %>
        <span class="d-inline-block" tabindex="0" data-bs-toggle="popover" data-bs-trigger="hover focus" data-bs-content="Did you loose/find something?">
          <!-- Button trigger modal -->
          <span id="createLostFoundBtn" class="btn" data-bs-toggle="modal" data-bs-target="#staticBackdroplostfound">
            <a class="btn-plus"><svg xmlns="http://www.w3.org/2000/svg" width="50" height="50" fill="currentColor" class="bi bi-plus-square" viewBox="0 0 25 25">
                <path d="M14 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z" />
                <path d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z" />
              </svg>
            </a>
          </span>
        </span>

        <!-- Modal -->
        <div class="modal fade" id="staticBackdroplostfound" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
          <div class="modal-dialog">
            <div class="modal-content p-3">
              <div class="modal-header">
                <h4 class="modal-title" id="staticBackdropLabel2">
                  Lost or Found?
                </h4>
                <button type="button" id="btnClose3" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
              </div>
              <div id="lostForm" class="modal-body">
                <form id="form-lost" class="mb-5" action="/playgrounds/<%= playground._id %>/lost-found" method="post">
                  <input type="hidden" name="_csrf" value="<%= csrftoken %>">
                  <div class="form-check">
                    <input class="form-check-input" type="radio" name="lost_found[status]" id="radioLost" value="lost" checked />
                    <label class="form-check-label" for="radioLost">
                      Lost
                    </label>
                  </div>
                  <div class="form-check">
                    <input class="form-check-input" type="radio" name="lost_found[status]" id="radioFound" value="found" />
                    <label class="form-check-label" for="radioFound">
                      Found
                    </label>
                  </div>
                  <label class="form-check-label mt-2 mb-0" for="lostDate">
                    Date
                  </label>
                  <input id="lostDate" name="lost_found[date]" type="date" class="w-100 mt-0 mb-2 p-2 br-10" required />
                  <label class="form-check-label mb-0" for="lostItem">
                    Item
                  </label>
                  <input id="lostItem" name="lost_found[title]" type="text" class="w-100 mt-0 mb-2 p-2 br-10" placeholder="What did you loose or find?" required />
                  <textarea id="lostItemLong" name="lost_found[description]" class="w-100 mt-0 mb-2 p-2 br-10" placeholder="describe the item in a few words" cols="30" rows="2" required></textarea>

                  <label class="form-check-label mb-0" for="lostContact">
                    Contact
                  </label>
                  <input id="lostContact" name="lost_found[contact]" type="text" class="w-100 mt-0 mb-2 p-2 br-10" placeholder="e.g. phone number or email" required />

                  <h5 class="mt-3 mb-2">
                    Upload a picture of the item!
                  </h5>
                  <input class="form-control" type="file" id="lostPhoto" onchange="preview2()" />
                  <img id="frame2" src="" class="img-fluid mt-4" />
                </form>
              </div>
              <span>
                <h3 class="text-center p-5" id="submitMessage2">
                  Thank you! Your entry has been added!
                </h3>
              </span>
              <div class="modal-footer">
                <button type="button" id="btnClose4" class="btn btn-secondary" data-bs-dismiss="modal">
                  Close
                </button>
                <button id="btnSubmit2" type="submit" class="btn btn-primary" form="form-lost">
                  Send
                </button>
              </div>
            </div>
          </div>
        </div>
        <% } %>


        <% if(!playground.lost_found.length) { %>
        <div style="height: 60px; text-align: center;">
          No Entries
        </div>
        <% } else { %>
        <div class="card">
          <ul>
            <% playground.lost_found.forEach(function(lostFound, i) { %>
            <li>
              <div class="row link" type="button" data-bs-toggle="collapse" href="#item<%= i %>-collapse" role="button" aria-expanded="false" aria-controls="item<%= i %>-collapse">
                <div id="<%= lostFound.status === 'lost' ? "statuslost" : "statusfound"%>" class="col-2 text-center align-self-center">
                  <%= lostFound.status.toUpperCase() %>
                </div>

                <div class="col text-center align-self-center">
                  <h5 id="theItem" class="heading">
                    <%= lostFound.title %>
                  </h5>

                </div>

                <div class="col-3">
                  <img class="thumbnail" src="https://images.unsplash.com/photo-1580250642511-1660fe42ad58?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1025&q=80" alt="" />
                </div>
                <% if(isAuthenticated && currentUser._id.toString()===lostFound.author.id.toString()) { %>
                <div class="col-2 align-self-center">
                  <button id="editLostFoundButton" data-id="<%= lostFound._id %>" class="btn bi bi-pencil editLostFoundBtn inline-block"></button>
                  <label class="d-none d-md-inline" for="editLostFoundButton">Edit</label>
                  <form action="/playgrounds/<%= playground._id %>/lost-found/<%= lostFound._id %>?_method=DELETE" method="post">
                    <input type="hidden" name="_csrf" value="<%= csrftoken %>">
                    <button id="deleteLostFoundButton" class="btn bi bi-trash3 inline-block"></button>
                    <label class="d-none d-md-inline" for="deleteLostFoundButton">Delete</label>
                  </form>
                </div>
                <% } %>
                <div class="row collapse mt-3" id="item<%= i %>-collapse">
                  <hr>
                  <div class="col-2 text-center align-self-center"><img src="https://i.imgur.com/hczKIze.jpg" height="50" width="50" class="rounded-circle" />
                    <p> <%= lostFound.author.name %></p>
                  </div>
                  <div class="col text-justify align-self-center ">
                    <%= lostFound.description %>
                    <hr>
                    <p><i class="fa-solid fa-address-card"></i> <%= lostFound.contact %></p>
                  </div>
                  <div class="col-3 text-center align-self-center"><%= (new Date(lostFound.date)).toDateString() %></div>
                </div>
              </div>
            </li>
            <% }); %>
          </ul>

        </div>
        <% } %>
      </div>

      <div class="w-100 d-none d-md-block d-xl-none"></div>
      <div class="gx-5 col col-xl-6 order-md-4 order-xl-3">
        <div class="text-left mt-4 mb-3">
          <h3 id="review-header">REVIEWS (<%= playground.reviews.length %>)</h3>
        </div>
        <% if(playground.reviews.length) { %>
        <ul>
          <% playground.reviews.forEach(function(review, i) { %>
          <li>
            <div id="review-<%= i %>" class="card p-3 mb-2">
              <div class="d-flex flex-row">
                <img src="https://i.imgur.com/dwiGgJr.jpg" height="60" width="60" class="rounded-circle" />
                <div class="d-flex flex-column ms-2">
                  <h5 class="mb-1 text-primary col"><%= review.author.name %></h5>

                  <span class="review-rating">
                    <% for(let k = 1; k <= 5; k++) {
                        if(k <= review.rating) { %>
                    <i class="bi bi-star-fill"></i>
                    <% } else { %>
                    <i class="bi bi-star"></i>
                    <% }} %>
                  </span>
                  <p class="comment-text"><%= review.body %></p>
                </div>

                <% if(isAuthenticated && currentUser._id.toString() === review.author.id.toString()) { %>
                <div class="ms-5 col align-self-center">
                  <button id="editReviewBtn-<%= i %>" class="btn bi bi-pencil editReviewBtn inline-block"></button>
                  <label class="d-none d-md-inline" for="editReviewBtn-<%= i %>">Edit</label>
                  <form action="/playgrounds/<%= playground._id %>/reviews/<%= review._id %>?_method=DELETE" method="post">
                    <input type="hidden" name="_csrf" value="<%= csrftoken %>">
                    <button id="deleteReviewBtn-<%= i %>" class="btn bi bi-trash3 inline-block"></button>
                    <label class="d-none d-md-inline" for="deleteReviewBtn-<%= i %>">Delete</label>
                  </form>
                </div>
                <% } %>

              </div>
              <div class="d-flex justify-content-between">
                <div class="d-flex flex-row gap-3 align-items-center">
                  <div class="d-flex align-items-center">
                    <i class="bi bi-heart<%= isAuthenticated && review.likes.includes(currentUser._id) ? "-fill" : "" %>" type="button">
                    </i>
                    <span class="ms-1 fs-10">Like</span>
                  </div>
                  <div class="d-flex align-items-center">
                    <i class="bi bi-chat-right-text" type="button" data-bs-toggle="collapse" href="#comment-collapse-<%= i %>" role="button" aria-expanded="false" aria-controls="comment-collapse-<%= i %>"></i>
                    <span class="ms-1 fs-10">Comment</span>
                  </div>
                </div>
                <div class="d-flex flex-row">
                  <span class="text-muted fw-normal fs-10"><%= (new Date(review.createdAt)).toLocaleString() %></span>
                </div>
              </div>
              <div class="row collapse" id="comment-collapse-<%= i %>">
                <form method="post" ; action="https://google.com">
                  <textarea cols="100" rows="1" class="mt-2 mb-2 p-2 br-10" placeholder="comment on this post"></textarea>
                  <div class="mb-3 d-flex flex-column">
                    <button id="postComment" type="submit" class="mt-2 btn btn-primary">POST
                    </button>
                  </div>
                </form>
              </div>
            </div>

            <% if(isAuthenticated && currentUser._id.toString() === review.author.id.toString()) { %>
            <div id="revEdit-<%= i %>" class="card p-3 mb-2 " hidden>
              <h4 class=" mb-3 comment-header"> Edit review </h4>
              <form id="editReviewForm-<%= i %>" action="/playgrounds/<%= playground._id %>/reviews/<%= review._id %>?_method=PUT&j=review-<%= i %>" method="POST">
                <input type="hidden" name="_csrf" value="<%= csrftoken %>">

                <div class="mb-3 d-flex flex-column">
                  <span class="rating">
                    <% for(let k = 5; k > 0; k--) { %>
                    <input type="radio" name="review[rating]" value="<%= k %>" id="edit-<%= k %>" <%= k === review.rating ? "checked" : "" %> /><label for="edit-<%= k %>">☆</label>
                    <% } %>

                  </span>

                  <textarea name="review[body]" cols="100" rows="3" class="mt-0 mb-2 p-2 br-10" placeholder="Please leave a review"><%= review.body %></textarea>

                  <div class="d-flex justify-content-end">
                    <button id="cancelRevEdit-<%= i %>" class="mt-2 me-2 btn btn-secondary cancelReviewEdit">
                      CANCEL
                    </button>
                    <button type="submit" class="mt-2 btn btn-primary">
                      SAVE
                    </button>
                  </div>
                </div>
              </form>
            </div>
            <% } %>
          </li>
          <% }); %>
        </ul>
        <% } %>

        <% if(isAuthenticated) { %>
        <div class="card p-3 mb-2 ">
          <h4 class=" mb-3 comment-header"> How do you like <%= playground.name %>? </h4>
          <form action="/playgrounds/<%= playground._id %>/reviews" method="POST">
            <input type="hidden" name="_csrf" value="<%= csrftoken %>">

            <div class="mb-3 d-flex flex-column">
              <span class="rating" id="starRating">
                <input type="radio" name="review[rating]" value="5" id="5" /><label for="5">☆</label>
                <input type="radio" name="review[rating]" value="4" id="4" /><label for="4">☆</label>
                <input type="radio" name="review[rating]" value="3" id="3" /><label for="3">☆</label>
                <input type="radio" name="review[rating]" value="2" id="2" /><label for="2">☆</label>
                <input type="radio" name="review[rating]" value="1" id="1" /><label for="1">☆</label>
              </span>

              <textarea id="review" name="review[body]" cols="100" rows="3" class="mt-0 mb-2 p-2 br-10" placeholder="Please leave a review"></textarea>

              <button id="postReview" type="submit" class="mt-2 btn btn-primary">
                POST REVIEW
              </button>
            </div>
          </form>
        </div>
        <% } %>

      </div>
    </div>

  </section>

  <script src="/javascript/utils.js"></script>
  <script>
    clearSearchData();
  </script>
  <script src="/javascript/detailpage.js"></script>

  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
</body>

</html>