        <div class="card p-3 mb-2">


            <div class="w-100 d-none d-md-block d-xl-none"></div>
            <div class="gx-5 col-12 col-xl-6 order-md-3 order-xl-4">
                <div class="row">
                    <div class="col">
                        <h3 id="event-header">UPCOMING EVENTS</h3>
                        <span class="d-inline-block" tabindex="0" data-bs-toggle="popover" data-bs-trigger="hover focus"
                            data-bs-content="Add an Event">
                            <!-- Button trigger modal -->
                            <span class="btn" data-bs-toggle="modal" data-bs-target="#staticBackdrop">
                                <a class="btn-plus"><svg xmlns="http://www.w3.org/2000/svg" width="50" height="50"
                                        fill="currentColor" class="bi bi-plus-square" viewBox="0 0 25 25">
                                        <path
                                            d="M14 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z" />
                                        <path
                                            d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z" />
                                    </svg> </a>
                            </span>
                        </span>
                        <!-- Modal -->
                        <div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false"
                            tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                            <div class="modal-dialog">
                                <div class="modal-content p-3">
                                    <div class="modal-header">
                                        <h4 class="modal-title" id="staticBackdropLabel">Add an Event</h4>
                                        <button type="button" id="btnClose1" class="btn-close btnClose"
                                            data-bs-dismiss="modal" aria-label="Close"></button>
                                    </div>
                                    <div id="eventForm" class="modal-body">
                                        <form class="mb-5" action="/eventdatabase">
                                            <label class="form-check-label" for="eventDate">
                                                Date
                                            </label>
                                            <input id="eventDate" type="date" class="w-100  mt-0 mb-2 p-2 br-10">
                                            <label class="form-check-label" for="eventTime">
                                                Time
                                            </label>
                                            <input id="eventTime" type="time" class="w-100  mt-0 mb-2 p-2 br-10">
                                            <label class="form-check-label" for="eventName">
                                                Name of the event
                                            </label>
                                            <input id="eventName" type="text" class="w-100 mt-0 mb-2 p-2 br-10"
                                                placeholder="name of the event">
                                            <label class="form-check-label" for="eventInfo">
                                                Info
                                            </label>
                                            <input id="eventInfo" type="text" class="w-100 mt-0 mb-2 p-2 br-10"
                                                placeholder="further information">
                                            <label class="form-check-label" for="eventLink">
                                                Add a link
                                            </label>
                                            <input id="eventLink" type="link" class="w-100 mt-0 mb-2 p-2 br-10"
                                                placeholder="add a Link">

                                            <h5 class=" mt-3 mb-2">Create a thumbnail for your event!</h5>
                                            <input class="form-control" type="file" id="eventPhoto"
                                                onchange="preview()">
                                            <img id="frame" src="" class="img-fluid mt-4" />

                                    </div>
                                    <span>
                                        <h3 class="text-center p-5" id="submitMessage">
                                            Thank you! Your event has been added.

                                        </h3>

                                    </span>

                                    <div class="modal-footer">
                                        <button type="button" id="btnClose2" class="btn btn-secondary btnClose"
                                            data-bs-dismiss="modal">Close</button>
                                        <button id="btnSubmit" type="button" class="btn btn-primary">Send</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <li class=" " data-bs-toggle="collapse" href="#event-collapse" role="button"
                            aria-expanded="false" aria-controls="event-collapse">

                            <div class="row link">


                                <div class="col-2 text-center">


